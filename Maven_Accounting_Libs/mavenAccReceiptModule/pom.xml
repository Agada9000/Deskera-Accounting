<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.mycompany</groupId>
  <artifactId>mavenAccReceiptModule</artifactId>
  <packaging>jar</packaging>
  <version>1.0</version>
  <name>mavenAccReceiptModule</name>
  <repositories>
    <repository>
      <id>test</id>
      <url>http://192.168.0.2/krawlerlib</url>
    </repository>
  </repositories>
  <dependencies>
     <dependency>
          <groupId>com.mycompany</groupId>
          <artifactId>mavenAccCommonLibs</artifactId>
          <version>1.0</version>
     </dependency>

    <dependency>
        <groupId>com.mycompany</groupId>
        <artifactId>mavenAccJournalEntryModule</artifactId>
        <version>1.0</version>
    </dependency>

    <dependency>
        <groupId>com.mycompany</groupId>
        <artifactId>mavenAccPaymentMethods</artifactId>
        <version>1.0</version>
    </dependency>

    <dependency>
        <groupId>com.mycompany</groupId>
        <artifactId>mavenAccInvoiceModule</artifactId>
        <version>1.0</version>
    </dependency>

    <dependency>
        <groupId>com.mycompany</groupId>
        <artifactId>mavenAccVendorModule</artifactId>
        <version>1.0</version>
    </dependency>

    <dependency>
      <groupId>com.mycompany</groupId>
      <artifactId>mavenAccMasterItems</artifactId>
      <version>1.0</version>
    </dependency>
  </dependencies>

  <build>
    <finalName>mavenAccReceiptModule</finalName>
    <plugins>
        <plugin>
            <artifactId>maven-compiler-plugin</artifactId>
            <version>2.0.2</version>
            <configuration>
                <source>1.6</source>
                <target>1.6</target>
            </configuration>
        </plugin>
    </plugins>
  </build>
  <properties>
     <netbeans.hint.deploy.server>Tomcat60</netbeans.hint.deploy.server>
  </properties>
</project>
