<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.krawler.hql.accounting.GoodsReceipt" table="goodsreceipt">
    <id column="id" name="ID">
      <generator class="uuid"/>
    </id>
    <!--property name="vendorInvoiceNumber" type="string">
      <column length="50" name="vendorinvoice" />
    </property-->
    <property name="goodsReceiptNumber" type="string">
      <column length="50" name="grnumber" not-null="true"/>
    </property>
    <property name="autoGenerated" type="true_false">
      <column default="false" name="autogen" not-null="true"/>
    </property>
    <property name="billFrom" type="string">
      <column length="255" name="billfrom" not-null="true"/>
    </property>
    <property name="shipFrom" type="string">
      <column length="255" name="shipfrom" not-null="true"/>
    </property>
    <property name="shipDate" type="timestamp">
      <column name="shipdate" not-null="true"/>
    </property>
    <property name="dueDate" type="timestamp">
      <column name="duedate" not-null="true"/>
    </property>
    <property name="memo" type="string">
      <column length="255" name="memo"/>
    </property>
    <property name="externalCurrencyRate" type="double">
      <column name="externalcurrencyrate" not-null="true" default='0'/>
    </property>
    <property name="deleted" type="true_false">
      <column name="deleteflag" not-null="true"/>
    </property>
    <property name="isExpenseType" type="true_false">
      <column name="isexpensetype" not-null="true" default="false"/>
    </property>
    <many-to-one cascade="all" class="com.krawler.hql.accounting.Discount" name="discount" unique="true"/>
    <set cascade="all" inverse="true" name="rows">
      <key column="goodsreceipt"/>
      <one-to-many class="com.krawler.hql.accounting.GoodsReceiptDetail"/>
    </set>
    <set cascade="all" inverse="true" name="expenserows">
      <key column="goodsreceipt"/>
      <one-to-many class="com.krawler.hql.accounting.ExpenseGRDetail"/>
    </set>
    <many-to-one class="com.krawler.hql.accounting.Tax" column="tax" name="tax"/>
    <many-to-one class="com.krawler.hql.accounting.Vendor" column="vendor" name="vendor"/>
    <many-to-one class="com.krawler.common.admin.KWLCurrency" column="currency" name="currency" not-null="true"/>
    <many-to-one class="com.krawler.hql.accounting.ExchangeRateDetails" column="exchangeratedetail" name="exchangeRateDetail" not-null="true"/>
    <many-to-one cascade="all" class="com.krawler.hql.accounting.JournalEntryDetail" column="centry" name="vendorEntry" unique="true"/>
    <many-to-one cascade="all" class="com.krawler.hql.accounting.JournalEntryDetail" column="sentry" name="shipEntry" unique="true"/>
    <many-to-one cascade="all" class="com.krawler.hql.accounting.JournalEntryDetail" column="oentry" name="otherEntry" unique="true"/>
    <many-to-one cascade="all" class="com.krawler.hql.accounting.JournalEntry" column="journalentry" name="journalEntry" not-null="true" unique="true"/>
    <many-to-one cascade="all" class="com.krawler.hql.accounting.JournalEntryDetail" column="taxentry" name="taxEntry" unique="true"/>
    <many-to-one class="com.krawler.common.admin.Company" name="company" not-null="true"/>
  </class>
</hibernate-mapping>
